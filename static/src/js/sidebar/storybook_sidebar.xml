<templates>
    <t t-name="storybook.StorybookSidebar" owl="1">

        <div class="o_storybook_sidebar h-100">
            <ul class="o_sidebar_tree p-3">
                <li>
                    <div class="o_storybook_module"><i class="fa fa-chevron-down"></i> Module 1 </div>
                </li>
                <li>
                    <ul class="pl-2">
                        <li> <div class="o_storybook_folder active"><i class="fa fa-folder-open"></i> Views </div> </li>
                        <li>
