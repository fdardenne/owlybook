                                <li>
                                    <div class="o_storybook_item active"><i class="fa fa-bookmark"></i> Story 1 </div>
                                </li>

                                <li>
                                    <div class="o_storybook_item"><i class="fa fa-bookmark"></i> Story 2 </div>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </li>

                <li>
                    <div class="o_storybook_module"><i class="fa fa-chevron-down"></i> Module 2 </div>
                </li>
                <li>
                    <ul class="pl-2">
                        <li>
                            <div class="o_storybook_folder"><i class="fa fa-folder-open"></i> Views </div>
                        </li>
                        <li>
                            <ul>
                                <li>
                                    <div class="o_storybook_item"><i class="fa fa-bookmark"></i> Story 1 </div>
                                </li>

                                <li>
                                    <div class="o_storybook_item"><i class="fa fa-bookmark"></i> Story 2 </div>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </li>


                <li>
                    <div class="o_storybook_module"><i class="fa fa-chevron-down"></i> Module 3 </div>
                </li>
                <li>
                    <ul class="pl-2">
                        <li>
                            <div class="o_storybook_folder"><i class="fa fa-folder"></i> Views </div>
